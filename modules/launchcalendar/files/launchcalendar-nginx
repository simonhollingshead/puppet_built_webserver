server {
    listen 80;
    listen [::]:80;
    root /srv/www/launchcalendar;
    index index.php;

    server_name launchcalendar.*;
    
    location ~ \.php$ {
        try_files $uri =404;
        include /etc/nginx/fastcgi_params;
        fastcgi_pass unix:/var/run/php-fastcgi/php-fastcgi.sock;
        fastcgi_index index.php;
        fastcgi_param SCRIPT_FILENAME /srv/www/launchcalendar/$fastcgi_script_name;
    }
    
    location / {
        try_files $uri $uri/ =404;
    }
}
